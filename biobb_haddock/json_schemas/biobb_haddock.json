{
    "_id": "biobb_haddock",
    "desc": "biobb_haddock is the Biobb module collection to compute classical molecular interaction potentials.",
    "github": "https://github.com/bioexcel/biobb_haddock",
    "readthedocs": "https://biobb-haddock.readthedocs.io/en/latest/",
    "conda": "https://anaconda.org/bioconda/biobb_haddock",
    "docker": "https://quay.io/biocontainers/biobb_haddock:3.7.8--pyhdfd78af_0",
    "singularity": "https://www.singularity-hub.org/collections/",
    "version": "3.7.8",
    "tools" : [
        {
            "block" : "CapriEval",
            "tool" : "Haddock CapriEval",
            "desc" : "The CapriEval module. Haddock CapriEval  module computes Capri evaluation for a docking.",
            "exec" : "capri_eval",
            "docs": "https://biobb-haddock.readthedocs.io/en/latest/haddock.html#module-capri_eval.capri_eval"
        },
        {
            "block" : "ClustFCC",
            "tool" : "Haddock ClustFCC",
            "desc" : "The ClustFCC module. Haddock ClustFCC  module computes clusters of structures using FCC.",
            "exec" : "clust_fcc",
            "docs": "https://biobb-haddock.readthedocs.io/en/latest/haddock.html#module-clust_fcc.clust_fcc"
        },
        {
            "block" : "EMRef",
            "tool" : "Haddock EMRef",
            "desc" : "The EMRef module. Haddock EMRef module computes an energy minimization refinement over selected structures.",
            "exec" : "em_ref",
            "docs": "https://biobb-haddock.readthedocs.io/en/latest/haddock.html#module-em_ref.em_ref"
        },
        {
            "block" : "FlexRef",
            "tool" : "Haddock FlexRef",
            "desc" : "The FlexRef module. Haddock FlexRef module computes a flexible refinement over selected structures.",
            "exec" : "flex_ref",
            "docs": "https://biobb-haddock.readthedocs.io/en/latest/haddock.html#module-flex_ref.flex_ref"
        },
        {
            "block" : "RigidBody",
            "tool" : "Haddock RigidBody",
            "desc" : "The RigidBody module. Haddock RigidBody  module compute rigid body docking between two molecules.",
            "exec" : "rigid_body",
            "docs": "https://biobb-haddock.readthedocs.io/en/latest/haddock.html#module-rigid_body.rigid_body"
        },
        {
            "block" : "SeleTop",
            "tool" : "Haddock SeleTop",
            "desc" : "The SeleTop module. Haddock SeleTop  module computes Selects the top  structures of a docking.",
            "exec" : "sele_top",
            "docs": "https://biobb-haddock.readthedocs.io/en/latest/haddock.html#module-sele_top.sele_top"
        },
        {
            "block" : "Topology",
            "tool" : "Haddock Topology",
            "desc" : "The Topology module. The Haddock Topology module creates a topology from a system to be used for docking.",
            "exec" : "topology",
            "docs": "https://biobb-haddock.readthedocs.io/en/latest/haddock.html#module-topology.topology"
        }
    ],
    "dep_pypi" : [
        "install_requires=['biobb_common==3.8.1']",
        "python_requires='>=3.7'"
    ],
    "dep_conda" : [
        "python >=3.7",
        "biobb_common ==3.8.1"
    ],
    "keywords" : [
        "haddock",
        "protein-protein",
        "interaction",
        "docking"
    ]
}
